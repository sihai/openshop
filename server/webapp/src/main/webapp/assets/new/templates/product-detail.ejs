<script type='text/javascript' src='https://apis.google.com/js/plusone.js'></script>
<script type='text/javascript' src='http://assets.pinterest.com/js/pinit.js'></script>
<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src='https://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document,'script','twitter-wjs');</script>

<div id='product-detail' >
    <div class='mask-detail'>	
    <div class='header-wrapper'>
		<div class='header'>
    		<a class='normal-link close' ><span>close</span></a>
    		<div class='header_desc'>
				<a class='first-item' class='normal-link disabled' title='<%shipping_cost_text%>' ><%shipping_cost_text%></a>
				<%read_all_the%>
				<a href='' class='normal-link disabled' title='<%shipping_and_delivery_information%>' ><%shipping_and_delivery_information%></a>
			</div>
		</div>
    </div>
    
	<div class='back-to-list-wrapper' >
    	<div class='back-to-list' >
    		<a href='/shop' class='iconos back grilla' ><span class='button-back-to-grid'><div class='back-to-grid-text'><%backtogrid%></div></span><span>back</span></a>
    	</div>
	</div>
	
	<div class='pager-wrapper' >
    	<div class='pager' >
    		<a class='normal-link iconos top disabled' ><span>prev_arrow</span></a>
    	    <div class='page-num' >1/3</div>
    	    <a class='normal-link iconos bottom' ><span>next_arrow</span></a>
    	</div>
	</div>
	
    <div class='page page-1 type-product <%type%>' >
    	<div class='page-background'>
    	<!--div style='background-image:url(<%init_folder%><%bg_name_image%>);' class='background-name'-->
    	<div class='background-name'>
	    	<div class='content-wrapper' >

				<div class='content' >
					
					<div class='image-holder'>
						<img title='<%prod_title%>' alt='<%prod_title%>' src='<%init_folder%><%main_image%>' />
	    			</div>

	    			<div class='menu' >
						<div class='menu-container'>
							<a class='normal-link product-info' ><%product_info%></a>&nbsp;/&nbsp;
							<a class='normal-link see-gallery' ><%see_the_gallery%></a>
						</div>
					</div>
					<div class='info-wrapper' style='display:none;' >
					<div class='info-holder' _style='display:none;' >
						<div class='close-iframe-over'></div>
						<div class='iconos close-iframe'><span></span></div>
							<div class='info-image-holder' >
								<img src='<%init_folder%><%main_image_thumb%>' />
							</div>
							<div class='info' >
								<h2>
								<%prod_title%>
								<!--img class='img_title_<%prod_title%>' title='<%prod_title%>' alt='<%prod_title%>' src='/website/images/product-names/<%image_title%>.png' /-->
								</h2>
								<div class='description'><p><%info_text%></p></div>
							</div>
							<div class='social-links clearfix' >
							
								<div class='fb-like' style='overflow:hidden!important;width:<%fc_width%>px!important;height:20px!important;'>
									<iframe src='//www.facebook.com/plugins/like.php?locale=<%fb_language%>&amp;href=<%url_share%>&amp;send=false&amp;layout=button_count&amp;width=60&amp;show_faces=false&amp;font&amp;colorscheme=light&amp;action=like&amp;height=20&amp;appId=287054418067421' scrolling='no' frameborder='0' style='border:none; overflow:hidden; width:<%fc_width%>px!important; height:20px;' allowTransparency='true'></iframe>
								</div>

								<div class='twitter' style='overflow:hidden;width:<%tw_width%>px!important;height:20px' >
									<a href='https://twitter.com/share' class='twitter-share-button' data-via='<%twitter_account%>' data-lang='<%current_language%>' data-text='<%twitter_text_share%>'>Twittear</a>
								</div>
								
								<div class='pinterest' >
									<a href='http://pinterest.com/pin/create/button/?url=<%url_share%>&amp;media=<%main_image%>' class='pin-it-button' count-layout='none'>
										<img border='0' src='//assets.pinterest.com/images/PinExt.png' title='Pin It' />
									</a>
								</div>

								<div class='googleplus' >
									<g:plusone size='medium' annotation='none' href='<%url_share%>'></g:plusone>
								</div>

							</div>
	    			</div>
	    			</div>

					
				
				</div>

			</div>
		</div>
		</div>
    </div>
    
    <div class='page page-2 type-gallery' >
    	<div class='page-background'>
	    	<div class='img-gallery-controls-wrapper' >
			<ul id='img-gallery-controls'>
				<%gallery_indicator%>
			</ul>
			</div>
			<div id='img-gallery'>
				<div id='scroller'>
					<ul id='thelist' >
						<%gallery_images%>
					</ul>
				</div>
			</div>
			<div class='pinterest_galery' >
				<a href='http://pinterest.com/pin/create/button/?url=<%url_share%>&amp;media=<%main_image%>' class='pin-it-button' count-layout='none'>
					<img border='0' src='//assets.pinterest.com/images/PinExt.png' title='Pin It' />
				</a>
			</div>
		</div>
    </div>
    
    <div class='page page-3 type-related' >
		<div class='page-background'>
			<div class='content-wrapper' >
				<div class='content <%related_items_class%>' >
					<div class='title'>
						<div class='line-1' ><%related_title_line1%></div>
						<div class='line-2' ><%related_title_line2%></div>
	    			</div>
					<%related_items_shoes%>
					<%related_items_separator%>
					<%related_items_accesories%>
				</div>
			</div>
		</div>
    </div>
    
    <div class='footer' >
    	<div class='footer-holder'>
    		<div class='title-price'>
        		<div class='block title' ><%prod_title%></div>
            	<div class='block price' ><span class='currency-symbol' ><%currency%></span><%prod_price%><span class='price-decimal'>,<%price_decimal%></span></div>
			</div>
			<div class='align_right clearfix'>
	            <div class='block size' >
	                <div class='pin-selected'></div>
	                <div class='size-title' ><%size_title%></div>
					<div class='size-num' ><%size_num%></div>
					<div class='widget-selector'>
						<div class='prev'></div>
					</div>
	            </div>
	            <div class='block quantity' >
	            	<div class='pin-selected'></div>
	            	<div class='quantity-title' ><%quantity_title%></div>
					<div class='quantity-num' ><%quantity_num%></div>
					<div class='widget-selector'>
						<div class='prev'></div>
					</div>
	            </div>
	            <div class='block add-to-cart' >
	            	<a id='add-to-cart' class='normal-link' ><%add_to_cart%></a>
	            </div>
			</div>
        </div>
    </div>
    </div>

</div>